files:
  "/etc/httpd/conf.d/01_rewrite.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      <VirtualHost *:80>
          DocumentRoot /var/www/html/src
          <Directory "/var/www/html/src">
              AllowOverride All
              Require all granted
          </Directory>
      </VirtualHost>
